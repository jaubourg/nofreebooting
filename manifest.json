{
    "manifest_version": 2,

    "name": "No Video Freebooting",
    "author": "jaubourg",
    "description": "Boycott Facebook videos until creators are properly protected and compensated.",
    "version": "0.5.0",

    "icons": {
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },

    "content_scripts": [ {
        "matches": [ "<all_urls>" ],
        "css": [
            "lib/style.css"
        ],
        "js": [
            "lib/script.js"
        ]
    } ]
}
